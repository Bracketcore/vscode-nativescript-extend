    "{NS} Tab View": {
        "prefix": "NStabview",
        "body": [
            "<TabView id=\"tabViewContainer\">",
            "    <TabView.items>",
            "        <TabViewItem title=\"Tab 1\">",
            "            <TabViewItem.view>",
            "                <Label text=\"This is Label in Tab 1\" />",
            "            </TabViewItem.view>",
            "        </TabViewItem>",
            "        <TabViewItem title=\"Tab 2\">",
            "            <TabViewItem.view>",
            "                <Label text=\"This is Label in Tab 2\" />",
            "            </TabViewItem.view>",
            "        </TabViewItem>",
            "    </TabView.items>",
            "</TabView>",
            ""
        ]
    },
    "{NS} Web View": {
